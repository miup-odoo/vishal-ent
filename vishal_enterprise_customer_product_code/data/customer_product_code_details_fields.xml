<?xml version='1.0' encoding='utf-8'?>
<odoo>
<record id="field_product_code_details_x_customer_product_code" model="ir.model.fields">
      <field name="name">x_customer_product_code</field>
      <field name="field_description">Customer Product Code</field>
      <field name="model_id" ref="vishal_enterprise_customer_product_code.model_x_customer_product_code_details"/>
      <field name="ttype">char</field>
      <field name="store" eval="True"/>
      <field name="required" eval="True"/>
      <field name="readonly" eval="False"/>
</record>

<record id="field_product_code_details_x_product_ids" model="ir.model.fields">
    <field name="name">x_product_id</field>
    <field name="field_description">Product Name</field>
    <field name="model_id" ref="vishal_enterprise_customer_product_code.model_x_customer_product_code_details"/>
    <field name="ttype">many2one</field>
    <field name="relation">product.template</field>
    <field name="on_delete">restrict</field>
    <field name="required" eval="True"/>
    <field name="readonly" eval="False"/>
</record>

<record id="field_product_code_details_x_customer_id" model="ir.model.fields">
  <field name="name">x_customer_id</field>
  <field name="field_description">Customer Name</field>
  <field name="model_id" ref="vishal_enterprise_customer_product_code.model_x_customer_product_code_details"/>
  <field name="ttype">many2one</field>
  <field name="relation">res.partner</field>
  <field name="required" eval="False"/>
  <field name="readonly" eval="False"/>
</record>
</odoo>
